@prefix : <http://www.w3id.org/agentic-ai/onto#> .
@prefix agento: <http://www.w3id.org/agentic-ai/onto#> .
@prefix ex: <http://www.w3id.org/agentic-ai/instances#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

### ======================================
### Instances (Entities)
### ======================================

### Researcher Agent
ex:researcher a agento:Agent ;
    agento:agentID "researcher_001" ;
    agento:agentRole "Researcher" ;
    dcterms:title "Research Agent" ;
    dcterms:description "You are an expert researcher analyzing data and trends" ;
    :hasGoal ex:goal_research ;
    :hasTask ex:task_research .

### Analyst Agent
ex:analyst a agento:Agent ;
    agento:agentID "analyst_001" ;
    agento:agentRole "Analyst" ;
    dcterms:title "Data Analyst" ;
    dcterms:description "You analyze research findings and provide insights" ;
    :hasGoal ex:goal_analyze ;
    :hasTask ex:task_analyze .

### Strategist Agent
ex:strategist a agento:Agent ;
    agento:agentID "strategist_001" ;
    agento:agentRole "Strategist" ;
    dcterms:title "Strategy Planner" ;
    dcterms:description "You develop strategic plans based on analysis" ;
    :hasGoal ex:goal_strategy ;
    :hasTask ex:task_strategy .

### Goals
ex:goal_research a agento:Goal ;
    dcterms:title "Research Goal" ;
    dcterms:description "Conduct comprehensive research on given topics" .

ex:goal_analyze a agento:Goal ;
    dcterms:title "Analysis Goal" ;
    dcterms:description "Analyze data and provide actionable insights" .

ex:goal_strategy a agento:Goal ;
    dcterms:title "Strategy Goal" ;
    dcterms:description "Develop strategic recommendations" .

### Tasks
ex:task_research a agento:Task ;
    dcterms:description "Research and gather relevant information" ;
    agento:taskExpectedOutput "Comprehensive research report" .

ex:task_analyze a agento:Task ;
    dcterms:description "Analyze research findings" ;
    agento:taskExpectedOutput "Detailed analysis with insights" .

ex:task_strategy a agento:Task ;
    dcterms:description "Create strategic plan" ;
    agento:taskExpectedOutput "Strategic recommendations document" .

### Workflow Pattern
ex:integration_workflow a agento:WorkflowPattern ;
    dcterms:title "CrewAI-LangGraph Integration Workflow" ;
    dcterms:description "Integrated workflow combining CrewAI crews with LangGraph routing" .
